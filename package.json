{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.28.0",
  "scripts": {
    "build": "vite build",
    "dev": "vite --host 0.0.0.0",
    "lint": "eslint . --debug",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "vue-tsc": "vue-tsc --noEmit"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@internationalized/date": "^3.11.0",
    "@nuxt/ui": "^4.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.0.1",
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@iconify-json/lucide": "^1.2.45",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^24.10.9",
    "@unhead/vue": "^2.1.2",
    "@vue/runtime-core": "^3.5.26",
    "@vueuse/core": "^14.1.0",
    "axios": "^1.13.2",
    "eslint": "^9.39.2",
    "eslint-plugin-better-tailwindcss": "^4.0.1",
    "husky": "^9.1.7",
    "hybridly": "^0.10.0-beta.11",
    "lint-staged": "^16.2.7",
    "tailwindcss": "^4.1.18",
    "taze": "^19.9.2",
    "typescript": "^5.9.3",
    "unplugin-icons": "^23.0.1",
    "validate-branch-name": "^1.3.2",
    "vite": "^7.3.1",
    "vue": "^3.5.26",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.2.2"
  },
  "lint-staged": {
    "**/*.{vue,ts}": "eslint --fix",
    "**/*.php": "vendor/bin/pint"
  },
  "validate-branch-name": {
    "pattern": "^(build|bugfix|bump|docs|experimental|feature|hotfix|merge|refactor|release|test)(/[A-Z]+\\-\\d+)?/[a-z0-9]+(-[a-z0-9]+)*(?:-\\d+)?$",
    "errorMsg": "Incorrect branch name, please refer to the regex documentation -> https://extendsclass.com/regex/f92c0fc"
  }
}
