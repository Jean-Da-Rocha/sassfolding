{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.9.0",
  "scripts": {
    "build": "vite build",
    "dev": "vite --host 0.0.0.0",
    "lint": "eslint . --debug",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "vue-tsc": "vue-tsc --noEmit"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@primevue/auto-import-resolver": "^4.3.3",
    "@primevue/icons": "^4.3.3",
    "primevue": "^4.3.3",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-primeui": "^0.6.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.4",
    "@types/node": "^22.14.1",
    "@unhead/vue": "^2.0.8",
    "@vue/runtime-core": "^3.5.13",
    "@vueuse/core": "^13.1.0",
    "axios": "^1.8.4",
    "eslint": "^9.25.1",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "husky": "^9.1.7",
    "hybridly": "^0.7.19",
    "lint-staged": "^15.5.1",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.4",
    "taze": "^19.0.4",
    "typescript": "^5.8.3",
    "validate-branch-name": "^1.3.2",
    "vite": "^6.3.2",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.10"
  },
  "postcss": {
    "plugins": {
      "@tailwindcss/postcss": {}
    }
  },
  "lint-staged": {
    "**/*.{vue,ts}": "eslint --fix",
    "**/*.php": "vendor/bin/pint"
  },
  "validate-branch-name": {
    "pattern": "^(feature|test|build|release|bugfix|hotfix|merge|experimental|bump)(/[A-Z]+\\-\\d+)?/[a-z]+(-[a-z]+)*(?:-\\d+)?$",
    "errorMsg": "Incorrect branch name, please refer to the regex documentation -> https://extendsclass.com/regex/bb50817"
  }
}
