{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.17.1",
  "scripts": {
    "build": "vite build",
    "dev": "vite --host 0.0.0.0",
    "lint": "eslint . --debug",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "vue-tsc": "vue-tsc --noEmit"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "primevue": "^4.3.9",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-primeui": "^0.6.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.4.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@iconify-json/heroicons": "^1.2.3",
    "@primevue/auto-import-resolver": "^4.3.9",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^22.18.6",
    "@unhead/vue": "^2.0.17",
    "@vue/runtime-core": "^3.5.21",
    "@vueuse/core": "^13.9.0",
    "axios": "^1.12.2",
    "eslint": "^9.36.0",
    "eslint-plugin-better-tailwindcss": "^3.7.9",
    "husky": "^9.1.7",
    "hybridly": "^0.8.3",
    "lint-staged": "^16.2.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
    "taze": "^19.7.0",
    "typescript": "^5.9.2",
    "unplugin-icons": "^22.3.0",
    "validate-branch-name": "^1.3.2",
    "vite": "^7.1.7",
    "vue": "^3.5.21",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.8"
  },
  "postcss": {
    "plugins": {
      "@tailwindcss/postcss": {}
    }
  },
  "lint-staged": {
    "**/*.{vue,ts}": "eslint --fix",
    "**/*.php": "vendor/bin/pint"
  },
  "validate-branch-name": {
    "pattern": "^(build|bugfix|bump|docs|experimental|feature|hotfix|merge|release|test)(/[A-Z]+\\-\\d+)?/[a-z0-9]+(-[a-z0-9]+)*(?:-\\d+)?$",
    "errorMsg": "Incorrect branch name, please refer to the regex documentation -> https://extendsclass.com/regex/f92c0fc"
  }
}
