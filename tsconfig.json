{
  "extends": "./.hybridly/tsconfig.json",
  "compilerOptions": {
    "paths": {
      "#build/ui": [
        "./node_modules/.nuxt-ui/ui"
      ],
      "@public/*": [
        "./public/*"
      ]
    },
    "noEmit": true,
    "sourceMap": false,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true
  }
}
